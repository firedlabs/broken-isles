{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "user"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputUsers",
      "databaseName": "firedlabs",
      "collectionName": "users",
      "connectionStringSetting": "firedlabs_DOCUMENTDB"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputUsers",
      "databaseName": "firedlabs",
      "collectionName": "users",
      "createIfNotExists": "true",
      "connectionStringSetting": "firedlabs_DOCUMENTDB"
    }
  ]
}
