{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
    "route": "func/user/auth/{token}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputUsers",
      "databaseName": "firedlabs",
      "collectionName": "users",
      "connectionStringSetting": "firedlabs_DOCUMENTDB"
    }
  ]
}
